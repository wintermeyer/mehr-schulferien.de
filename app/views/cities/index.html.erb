<h1>Listing cities</h1>

<table>
  <thead>
    <tr>
      <th>Federal state</th>
      <th>Zip code</th>
      <th>Name</th>
      <th>Slug</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @cities.each do |city| %>
      <tr>
        <td><%= city.federal_state %></td>
        <td><%= city.zip_code %></td>
        <td><%= city.name %></td>
        <td><%= city.slug %></td>
        <td><%= link_to 'Show', city %></td>
      </tr>
    <% end %>
  </tbody>
</table>
