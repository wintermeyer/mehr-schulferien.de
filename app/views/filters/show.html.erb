<% content_for :html_description do %><%= @html_description %><% end %>
<% content_for :html_title do %><%= @html_title %><% end %>

<% if @year %>
  <%= render partial: "years/one_year", locals: {year: @year, months: @months, federal_state: @federal_state, religions: @religions, religion: @religion, modus: @modus} %>
<% else %>
  <%= render partial: "federal_states/one_federal_state", locals: {months: @months, federal_state: @federal_state, religions: @religions, religion: @religion, modus: @modus, year: @year} %>
<% end %>
